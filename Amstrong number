import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int dig=String.valueOf(n).length();
        int sum=0;
        int t=n;
        while(t>0) {
            int d=t%10;
            sum+=Math.pow(d,dig);
            t/=10;
        }
        if(sum==n) {
            System.out.println("Amstrong number");
        }
        else {
            System.out.println("Not an amstrong number");
        }
    }
}
