import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String n=s.nextLine();
        String reversed="";
        for(int i=n.length()-1;i>=0;i--) {
            reversed+=n.charAt(i);
        }
        System.out.println(reversed);
    }
}
