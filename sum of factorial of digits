import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
        while(n>0) {
            int d=n%10;
            int fact=1;
            for(int i=1;i<=d;i++) {
                fact*=i;
            }
            sum+=fact;
            n/=10;
        }
         System.out.println(sum);
    }
}
