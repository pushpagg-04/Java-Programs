import java.util.Scanner; 
public class Main { 
    public static void main(String[] args) { 
        Scanner sc=new Scanner(System.in); 
        int n=sc.nextInt(); 
        int m=sc.nextInt(); 
        int x=n,y=m; 
        while(y!=0) { 
            int temp=y; 
            y=x%y; 
            x=temp; 
        } 
        int gcd=x; 
        int lcm=(n*m)/gcd; 
        System.out.println(lcm); 
    } 
}
