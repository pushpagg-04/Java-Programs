import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
        int t=n;
        while(t>0) {
            int d=t%10;
            sum=sum*10+d;
            t/=10;
        }
        System.out.println(sum);
    }
}
